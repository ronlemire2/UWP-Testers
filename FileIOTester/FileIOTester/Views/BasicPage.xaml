<Page x:Name="Basic"
    x:Class="FileIOTester.Views.BasicPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:FileIOTester.Views"
    xmlns:controls="using:FileIOTester.Controls"
    xmlns:mvvm="using:Prism.Windows.Mvvm"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mvvm:ViewModelLocator.AutoWireViewModel="True"
    mc:Ignorable="d">

    <Grid Background="{ThemeResource ApplicationPageBackgroundThemeBrush}"
          EntranceNavigationTransitionInfo.IsTargetElement="True">

        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" >
            <TextBlock Style="{ThemeResource TitleTextBlockStyle}" Text="Your App Content"/>
            <Button Content="Go to CommandBarPage" Command="{Binding GoToCommandBarPage}" />
        </StackPanel>

    </Grid>
</Page>
